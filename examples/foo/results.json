{
  "name": "results-seb-foo",
  "description": "LHS within the priors",
  "keywords": ["lhs"],
  "version": "0.0.0",
  "dependencies": {
    "model-seb-sir": "*"
  },
  "licenses": [
    {
      "type": "cc0",
      "url": "http://creativecommons.org/publicdomain/zero/1.0/"
    }
  ],

  "version": "0.0.0",

  "analysis": [
    {
      "name": "map",
      "data": [
        {
          "engines": {"ssm": "*"},
          "command": "smc",
          "args": ["ode", "--traj", "--next", "skip_", "--path", "$ROOT/data", "--id", "*"],
          "inputs": [
            {
              "resource": "lhs",
              "collection": true
            }
          ],
          "outputs": [
            {
              "path": "data/skip_*.json"
            },
            {
              "path": "data/X_*.csv",
              "save" : {
                "resource": "X_*"
              }
            }
          ]
        },

        {
          "engines": {"ssm": "*"},
          "command": "simplex",
          "args": ["ode", "--iter", "100", "--trace", "--next", "mle_", "--path", "$ROOT/data", "--id", "*"],
          "inputs": [
            {
              "path": "data/skip_*.json"
            }
          ],
          "outputs": [
            {
              "path": "data/trace_*.csv",
              "save" : {
                "resource": "trace_*",
                "path": "data/my_trace_*.csv"
              }
            },
            {
              "path": "data/mle_*.json"
            }
          ]
        }
      ]
    }
  ],

  "resources": [
    {
      "name": "lhs",
      "path": "data/lhs.json",
      "format": "json"
    }
  ]
}

