{
  "name": "ssm-tutorial-data",
  "version": "0.0.0",
  "description": "The data for the S|S|M tutorial",
  "repository": {
    "type": "git",
    "url": "git://github.com/standard-analytics/ssm-tutorial-data.git"
  },
  "files": [
    "data/data.csv"
  ],
  "keywords": [
    "SSM",
    "tutorial",
    "data"
  ],
  "author": {
    "name": "Sebastien Ballesteros"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/standard-analytics/ssm-tutorial-data/issues"
  },
  "homepage": "https://github.com/standard-analytics/ssm-tutorial-data",
  "resources": [
    {
      "name": "data",
      "path": "data/data.csv",
      "format": "csv",
      "schema": {
        "fields": [
          {
            "name": "date",
            "type": "date"
          },
          {
            "name": "cases",
            "type": "number"
          }
        ]
      }
    },
    {
      "name": "S",
      "format": "json",
      "description": "Number of susceptible",
      "data": {
        "distribution": "fixed",
        "value": 70000
      }
    },
    {
      "name": "I",
      "format": "json",
      "description": "Number of infectious",
      "data": {
        "distribution": "fixed",
        "value": 10
      }
    },
    {
      "name": "R",
      "format": "json",
      "description": "Number of recovered",
      "data": {
        "distribution": "fixed",
        "value": 929990
      }
    },
    {
      "name": "r0",
      "format": "json",
      "description": "basic reproduction number",
      "data": {
        "distribution": "uniform",
        "lower": 15,
        "upper": 35
      }
    },
    {
      "name": "pr_v",
      "description": "duration of infection",
      "format": "json",
      "data": {
        "distribution": "normal",
        "mean": 12.5,
        "sd": 3.8265,
        "lower": 0,
        "unit": "days"
      }
    },
    {
      "name": "rep",
      "description": "reporting rate",
      "format": "json",
      "data": {
        "distribution": "fixed",
        "value": 0.5
      }
    }
  ]
}
