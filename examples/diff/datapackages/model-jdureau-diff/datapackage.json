{
  "name": "model-jdureau-diff",
  "description": "SIR model with birth and death processes, diffusion on the transmission term and overdispersed binomial observation process",
  "keywords": ["SIR", "overdispersed binomial", "diffusion", "2 cities", "paris", "new york"],
  "dependencies": {
    "data-jdureau-test": "*"
  },
  "licenses": [
    {
      "type": "cc0",
      "url": "http://creativecommons.org/publicdomain/zero/1.0/"
    }
  ],

  "version": "0.0.0",

  "resources": [

    {
      "name": "populations",
      "data": [
        {"id": "paris", "composition": ["S_paris", "I_paris", "R_paris"], "remainder": {"id": "R_paris", "pop_size": "N_paris"}},
        {"id": "nyc",   "composition": ["S_nyc", "I_nyc", "R_nyc"],       "remainder": {"id": "R_nyc",   "pop_size": "N_nyc"}}
      ]
    },

    {
      "name": "reactions",
      "data":  [
        {"from": "U",       "to": "S_nyc",   "rate": "mu_b_nyc*N_nyc",             "description":"birth"},
        {"from": "U",       "to": "S_paris", "rate": "mu_b_paris*N_paris",         "description":"birth"},

        {"from": "S_nyc",   "to": "I_nyc",   "rate": "r0_nyc/N_nyc*v*I_nyc",       "description":"infection", "keywords": ["transmission"], "tracked": ["all_inc","nyc_inc"]},
        {"from": "S_paris", "to": "I_paris", "rate": "r0_paris/N_paris*v*I_paris", "description":"infection", "keywords": ["transmission"], "tracked": ["all_inc"]},

        {"from": "I_nyc",   "to": "R_nyc",   "rate": "correct_rate(v)",            "description":"recovery"},
        {"from": "I_paris", "to": "R_paris", "rate": "correct_rate(v)",            "description":"recovery"},

        {"from": "S_nyc",   "to": "U",       "rate": "mu_d_nyc",                   "description":"death"},
        {"from": "S_paris", "to": "U",       "rate": "mu_d_paris",                 "description":"death"},

        {"from": "I_nyc",   "to": "U",       "rate": "mu_d_nyc",                   "description":"death"},
        {"from": "I_paris", "to": "U",       "rate": "mu_d_paris",                 "description":"death"}
      ]
    },

     {
      "name": "sde",
      "data": {
        "drift": [
          {"id": "r0_paris", "f": 0.0, "transformation": "log(r0_paris)"},
          {"id": "r0_nyc",   "f": 0.0, "transformation": "log(r0_nyc)"}
        ],
        "dispersion":[
          ["vol", 0],
          [0, "vol"]
        ]
      }
    },


    {
      "name": "observations",
      "data": [
        {
          "id": "all_CDC_inc",
          "data": {
            "path": "datapackages/data-sballest-noise/data/all_CDC_inc"
          },
          "start": "2012-07-26",
          "pdf": {
            "distribution": "discretized_normal",
            "mean": "rep_all_CDC_inc * prop_all_CDC_inc * all_inc",
            "sd": "sqrt(rep_all_CDC_inc * ( 1.0 - rep_all_CDC_inc ) * prop_all_CDC_inc * all_inc + ( rep_all_CDC_inc * phi * prop_all_CDC_inc * all_inc )^2 )"
          }
        },

        {
          "id": "paris_CDC_prev",
          "data": {
            "path": "datapackages/data-sballest-noise/data/paris_CDC_prev"
          },
          "start": "2012-07-26",
          "pdf": {
            "distribution": "discretized_normal",
            "mean": "rep_paris_CDC_prev * prop_paris_CDC_prev * I_paris",
            "sd": "sqrt(rep_paris_CDC_prev * ( 1.0 - rep_paris_CDC_prev ) * prop_paris_CDC_prev * I_paris + ( rep_paris_CDC_prev * phi * prop_paris_CDC_prev * I_paris)^2 )"
          }
        },

        {
          "id": "all_google_inc",
          "data": {
            "path": "datapackages/data-sballest-noise/data/all_google_inc"
          },
          "start": "2012-07-26",
          "pdf": {
            "distribution": "discretized_normal",
            "mean": "rep_all_google_inc * prop_all_google_inc * all_inc",
            "sd": "sqrt(rep_all_google_inc * ( 1.0 - rep_all_google_inc ) * prop_all_google_inc * all_inc + ( rep_all_google_inc * phi * prop_all_google_inc * all_inc )^2 )"
          }
        },

        {
          "id": "nyc_CDC_inc",
          "data": {
            "path": "datapackages/data-sballest-noise/data/nyc_CDC_inc"
          },
          "start": "2012-07-26",
          "pdf": {
            "distribution": "discretized_normal",
            "mean": "rep_nyc_CDC_inc * prop_nyc_CDC_inc * nyc_inc",
            "sd": "sqrt(rep_nyc_CDC_inc * ( 1.0 - rep_nyc_CDC_inc ) * prop_nyc_CDC_inc * nyc_inc + ( rep_nyc_CDC_inc * phi * prop_nyc_CDC_inc * nyc_inc )^2 )"
          }
        }
      ]
    },

    {
      "name": "parameters",
      "data": [

        {
          "id": "S_paris",
          "description": "Number of susceptible",
          "transformation": "pr_S_paris*N_paris",
          "to_prior": "S_paris/N_paris",
          "prior": {
            "id": "pr_S_paris",
            "description": "Proportion of susceptible",
            "distribution": "fixed", 
            "value":0.07
          }
        },

        {
          "id": "S_nyc",
          "description": "Proportion of susceptible",
          "transformation": "pr_S_paris*N_nyc"
        },

        {
          "id": "I_paris",
          "description": "Number of infectious",
          "keywords": ["infectious"],
          "transformation": "pr_I_paris*N_paris",
          "to_prior": "I_paris/N_paris",
          "prior": {
            "id": "pr_I_paris",
            "description": "Proportion of infectious",
            "distribution": "uniform",
            "lower": 1e-6, 
            "upper": 1e-4
          }
        },

	{
          "id": "I_nyc",
          "description": "Proportion of susceptible",
          "transformation": "pr_I_paris*N_nyc"
        },

	{
          "id": "r0_paris",
          "description": "basic reproduction number",
          "prior": {
            "distribution": "normal",
            "mean": 25.0,
            "sd": 5.102,
	    "lower": 0
          }
        },

	{
          "id": "r0_nyc",
          "description": "basic reproduction number",
          "prior": {
            "distribution": "normal",
            "mean": 25.0,
            "sd": 5.102,
	    "lower": 0
          }
        },

	{
          "id": "vol",
          "description": "volatility of the diffusion",
          "prior": {
            "distribution": "uniform",
            "lower": 0.0,
            "mean": 0.25,
	    "sd": 0.1276
          }
        },

	{
          "id": "v",
          "description": "recovery rate",          
          "transformation": "1/pr_v",
          "prior": {
            "id": "pr_v",
            "description": "duration of infection",
            "distribution": "normal",
            "mean": 12.5,
            "sd": 3.8265,
            "unit": "days"
          }
        },

        {
          "id": "rep_all_CDC_inc",
          "description": "reporting rate",
          "prior": {
            "distribution": "uniform",
            "lower": 0.5,
            "upper": 0.8
          }
        },

	{
          "id": "rep_all_google_inc",
          "description": "reporting rate",
	  "transformation": "rep_all_CDC_inc"
        },

	{
          "id": "rep_nyc_CDC_inc",
          "description": "reporting rate",
	  "transformation": "rep_all_CDC_inc"
        },

	{
          "id": "paris_CDC_prev",
          "description": "reporting rate",
	  "transformation": "rep_all_CDC_inc"
        },

        {
          "id": "phi",
          "description": "over-dispertion",
          "prior": {
            "distribution": "fixed", 
            "value": 0.1
          }
        },

        {
          "id": "mu_b_paris",
          "description": "birth rate",
          "prior": {
            "path": "datapackages/data-sballest-noise/mu_b/paris"
          }
        },

        {
          "id": "mu_b_nyc",
          "description": "birth rate",
          "prior": {
            "path": "datapackages/data-sballest-noise/mu_b/nyc"
          }
        },

        {
          "id": "mu_d_paris",
          "description": "death rate",
          "prior": {
            "path": "datapackages/data-sballest-noise/mu_d/paris"
          }
        },

        {
          "id": "mu_d_nyc",
          "description": "death rate",
          "prior": {
            "path": "datapackages/data-sballest-noise/mu_d/nyc"
          }
        },

        {
          "id": "N_paris",
          "description": "population size",
          "prior": {
            "path": "datapackages/data-sballest-noise/N/paris"
          }
        },

        {
          "id": "N_nyc",
          "description": "population size",
          "prior": {
            "path": "datapackages/data-sballest-noise/N/nyc"
          }
        },

        {
          "id": "prop_all_CDC_inc",
          "description": "proportion of the population under surveillance",
          "prior": {
            "path": "datapackages/data-sballest-noise/prop/all_CDC_inc"
          }
        },

        {
          "id": "prop_paris_CDC_prev",
          "description": "proportion of the population under surveillance",
          "prior": {
            "path": "datapackages/data-sballest-noise/prop/paris_CDC_prev"
          }
        },

        {
          "id": "prop_all_google_inc",
          "description": "proportion of the population under surveillance",
          "prior": {
            "path": "datapackages/data-sballest-noise/prop/all_google_inc"
          }
        },

        {
          "id": "prop_nyc_CDC_inc",
          "description": "proportion of the population under surveillance",
          "prior": {
            "path": "datapackages/data-sballest-noise/prop/nyc_CDC_inc"
          }
        }
      ]
    }
  ]
}
