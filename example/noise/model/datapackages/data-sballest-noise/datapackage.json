{
  "name": "data-sballest-noise",
  "description": "Canonical example data set for PLOM.",
  "licenses": [
    {
      "type": "cc0",
      "url": "http://creativecommons.org/publicdomain/zero/1.0/"
    }
  ],
  "keywords": ["Time series", "simulated data"],
  "version": "0.0.0",
  "sources": [
    {
      "name": "PLOM.IO",
      "web": "http://plom.io"
    }
  ],

  "resources": [
    {
      "name": "data",
      "path": "data/data.csv",
      "format": "csv",
      "schema": {
        "fields": [
          {
            "name": "date",
            "type": "date"
          },
          {
            "name": "all_CDC_inc",
            "type": "number",
            "description": "Incidence in both cities as reported by the CDC"
          },
          {
            "name": "all_google_inc",
            "type": "number",
            "description": "Incidence in both cities as reported by the google"
          },
          {
            "name": "nyc_CDC_inc",
            "type": "number",
            "description": "Incidence in nyc as reported by the CDC"
          },
          {
            "name": "paris_CDC_prev",
            "type": "number",
            "description": "Prevalence in paris as reported by the CDC"
          }
        ]
      }
    },


    {
      "name": "prop",
      "path": "data/prop.csv",
      "description": "proportion of the population under surveillance",
      "schema": {
        "fields": [
          {
            "name": "date",
            "type": "date"
          },
          {
            "name": "all_CDC_inc",
            "type": "number"
          },
          {
            "name": "all_google_inc",
            "type": "number"
          },
          {
            "name": "nyc_CDC_inc",
            "type": "number"
          },
          {
            "name": "paris_CDC_prev",
            "type": "number"
          }
        ]
      }
    },

    {
      "name": "N",
      "path": "data/N.csv",
      "description": "population size",
      "schema": {
        "fields": [
          {
            "name": "date",
            "type": "date"
          },
          {
            "name": "paris",
            "type": "number",
            "description": "population size in Paris"
          },
          {
            "name": "nyc",
            "type": "number",
            "description": "population size in NYC"
          }
        ]
      }
    },

    {
      "name": "mu_b",
      "path": "data/mu_b.csv",
      "description": "birth rates",
      "schema": {
        "fields": [
          {
            "name": "date",
            "type": "date",
            "unit": "weeks"
          },
          {
            "name": "paris",
            "type": "number",
            "unit": "weeks"
          },
          {
            "name": "nyc",
            "type": "number",
            "unit": "weeks"
          }
        ]
      }
    },

    {
      "name": "mu_d",
      "path": "data/mu_d.csv",
      "description": "death rates",
      "schema": {
        "fields": [
          {
            "name": "date",
            "type": "date"
          },
          {
            "name": "paris",
            "type": "number",
            "unit": "weeks"
          },
          {
            "name": "nyc",
            "type": "number",
            "unit": "weeks"
          }
        ]
      }
    }
  ]
}
