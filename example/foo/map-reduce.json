{
  "name": "mr-seb-foo",
  "description": "LHS within the priors",
  "keywords": ["trial"],
  "version": "0.0.0",
  "dependencies": {
    "model-seb-sir": "*"
  },
  "licenses": [
    {
      "type": "cc0",
      "url": "http://creativecommons.org/publicdomain/zero/1.0/"
    }
  ],

  "version": "0.0.0",
 
  "map": [
    {
      "prog": "simul",
      "engines": {"ssm": "*"}
      "commands": ["ode"],
      "options": {
        "end": "2043-07-25",
        "traj": true
      },
      "inputs": [
        {
          "resource": "lhs",
          "collection": true
        }
      ],
      "outputs": [
        {
          "files": "data/skip_*.json"
        },
        {
          "files": "data/X_*.json",
          "save":true,
        },
      ]
    },
    {
      "prog": "simplex",
      "engines": {"ssm": "*"}
      "commands": ["ode"],
      "options": { 
        "n_iter": 10000,
        "trace": true
      },
      "inputs": [
        {
          "files": "data/skip_*.json"
        }
      ],
      "outputs": [
        {
          "files": "data/trace_*.json",
          "save":true
        },
        {
          "files": "data/mle_*.json"
        }
      ]
    }
  ],


  "resources": [
    {
      "name": "X_0",
      "path": "data/trace_0.csv",
      "format": "csv"
    },
    {
      "name": "X_1",
      "path": "data/trace_0.csv",
      "format": "csv"
    },

    {
      "name": "trace_0",
      "path": "data/trace_0.csv",
      "format": "csv"
    },
    {
      "name": "trace_1",
      "path": "data/trace_1.csv",
      "format": "csv"
    }

  ]
}
