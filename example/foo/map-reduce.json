{
  "name": "mr-seb-foo",
  "description": "LHS within the priors",
  "keywords": ["trial"],
  "version": "0.0.0",
  "dependencies": {
    "model-seb-sir": "*"
  },
  "licenses": [
    {
      "type": "cc0",
      "url": "http://creativecommons.org/publicdomain/zero/1.0/"
    }
  ],

  "version": "0.0.0",

  "resources": [
    {
      "name": "inputs",
      "path": "data/lhs.json",
      "format": "json"
    },

    {
      "name": "map",
      "data": [
        {
          "prog": "simul",
          "version": "0.0.1",
          "commands": ["ode"],
          "options": {
            "end": "2043-07-25",
            "traj": true
          },
          "resources": ["X"]
        },
        {
          "prog": "simplex",
          "version": "0.0.1",
          "commands": ["ode"],
          "options": { 
            "n_iter": 10000,
            "trace": true
          },
          "resources": ["traces", "mle"]
        }
      ]
    },

    {
      "name": "reduce",
      "data": []
    },


    {
      "name": "traces",
      "path": ["data/trace_0.csv", "data/trace_1.csv", "data/trace_2.csv", "data/trace_3.csv", "data/trace_4.csv"],
      "format": "csv",
      "schema": {
        "fields": []
      }
    },

    {
      "name": "mle",
      "path": ["data/theta_0.json", "data/theta_1.json", "data/theta_2.json", "data/theta_3.json", "data/theta_4.json"],
      "format": "json",
      "schema": {
        "fields": []
      }
    }
  ]
}
